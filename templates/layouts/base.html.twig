<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Virtual Exhibition!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app')}}
        {% endblock %}
    </head>
    <body role ="main" class="container">
        {{ include('layouts/partials/_flash_message.html.twig') }}

        {{ include('layouts/partials/_nav.html.twig') }}

        <main>
            {% block body %}{% endblock %}        
        </main>

        

        {{ include('layouts/partials/_footer.html.twig') }}

        {% block javascripts %}
            {{ encore_entry_script_tags('app')}}
        {% endblock %}
    </body>
</html>
